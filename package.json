{
  "version": "1.2.2",
  "name": "crm-powerbi-viewer",
  "displayName": "Power BI Viewer (v1.2-rc)",
  "description": "Embed tiles and reports from Power BI into Dynamics 365 Sales and Service Forms and Dashboards.",
  "homepage": "http://crm-powerbi-viewer.heiigjen.com/",
  "license": "MIT",
  "author": "Trond Aarskog (trond<alphahere>heiigjen.com) from Heiigjen Solutions",
  "bugs": {
    "url": "https://github.com/taarskog/crm-powerbi-viewer/issues"
  },
  "private": true,
  "keywords": [
    "xrm",
    "crm",
    "d365",
    "dynamics 365",
    "powerbi"
  ],
  "scripts": {
    "watch": "gulp watch --env debug",
    "build": "gulp build --env production && powershell ./tools/packSolution.ps1",
    "build-debug": "gulp build --env debug && powershell ./tools/packSolution.ps1",
    "build-sample": "gulp build:javascript --env debug",
    "deploy": "npm run build && powershell ./tools/deployFiles.ps1",
    "deploy-debug": "npm run build-debug && powershell ./tools/deployFiles.ps1",
    "deploy-sample": "npm run build-sample && powershell ./tools/deployFiles.ps1",
    "clean": "gulp clean"
  },
  "devDependencies": {
    "@types/adal-angular": "latest",
    "@types/browser-sync": "latest",
    "@types/browserify": "latest",
    "@types/clipboard": "latest",
    "@types/gulp": "latest",
    "@types/merge-stream": "latest",
    "@types/xrm": "latest",
    "ansi-colors": "4.1.1",
    "@babel/core": "7.13.13",
    "@babel/preset-env": "7.13.12",
    "babelify": "10.0.0",
    "browser-sync": "^2.26.14",
    "browserify": "17.0.0",
    "bs-html-injector": "3.0.3",
    "del": "6.0.0",
    "tslint": "6.1.3",
    "event-stream": "4.0.1",
    "fancy-log": "1.3.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "4.3.0",
    "gulp-css-svg": "1.3.1",
    "gulp-header": "2.0.9",
    "gulp-html-replace": "1.6.2",
    "gulp-htmlmin": "5.0.1",
    "gulp-if": "3.0.0",
    "gulp-newer": "1.4.0",
    "gulp-plumber": "1.2.1",
    "gulp-preprocess": "3.0.3",
    "gulp-sass": "4.1.0",
    "gulp-sourcemaps": "3.0.0",
    "gulp-tslint": "8.1.4",
    "gulp-uglify": "3.0.2",
    "merge-stream": "2.0.0",
    "minimist": "1.2.6",
    "tsify": "5.0.2",
    "typescript": "4.2.3",
    "vinyl-buffer": "1.0.1",
    "vinyl-source-stream": "2.0.0"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "adal-angular": "1.0.15",
    "powerbi-client": "2.17.2",
    "handlebars": "4.7.7",
    "clipboard": "2.0.8"
  }
}
